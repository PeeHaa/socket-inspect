{
    "name": "peehaa/socket-inspect",
    "description": "Reverse engineering and socket inspection tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "amphp/http-server": "^0.8.3",
        "amphp/http-server-router": "^1",
        "amphp/http-server-static-content": "^1.0.1",
        "amphp/socket": "^0.10",
        "amphp/websocket-server": "dev-master",
        "myclabs/php-enum": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "slevomat/coding-standard": "^4.8",
        "squizlabs/php_codesniffer": "^3.3.1"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\SocketInspect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\SocketInspectTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
