{
    "name": "peehaa/socket-inspect",
    "description": "Reverse engineering and socket inspection tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "amphp/socket": "^0.10",
        "myclabs/php-enum": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "slevomat/coding-standard": "^4.8",
        "squizlabs/php_codesniffer": "^3.3.1"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\SocketInspect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\SocketInspectTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
